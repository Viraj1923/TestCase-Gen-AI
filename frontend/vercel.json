{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/files", "dest": "/api/index.js" },
    { "src": "/file-contents", "dest": "/api/index.js" },
    { "src": "/generate-test-summaries", "dest": "/api/index.js" },
    { "src": "/generate-code", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
